{
    "version": 2,
    "alias": "maze.maxquinn.nz",
    "builds": [
        { "src": "api/**/*.js", "use": "@now/node" },
        { "src": "next.config.js", "use": "@now/next" }
    ],
    "routes": [
      {
        "src": "/api/highscores",
        "dest": "api/highscores"
      }
    ],
    "env": {
      "MYSQL_HOST": "@mysql_host",
      "MYSQL_USER": "@mysql_user",
      "MYSQL_PASSWORD": "@mysql_password",
      "MYSQL_DATABASE": "@mysql_database"
    }
}
